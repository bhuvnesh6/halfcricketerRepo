  <body>
    <h1 class="nav justify-content-center bg-body-black"  style="background-color: #030303; color: white; padding: 10px;">Most viewed articles on halfcricketer ever</h1>
    <div class="container" style="padding: 15px;">
        <% viewd.forEach(data => { %> 
   <a href="articles/<%=data._id%>" style="text-decoration: none;"> <div class="card mb-3 .Mviewd">
        <img src="<%=data.thumbnail%>" class="card-img" style="height: 400px;" alt="...">
        <div class="card-body">
          <h5 class="card-title"><%=data.title%></h5>
          <p class="card-text"><%=data.discription%></p>
          <p class="card-text"><small class="text-body-secondary">views : <%=data.views%></small></p>
        </div>
      </div>  
    </a>
      <%})%>
    </div>
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.2/dist/js/bootstrap.bundle.min.js" integrity="sha384-C6RzsynM9kWDrMNeT87bh95OGNyZPhcTNXj1NW7RuBCsyN/o0jlpcV8Qyq46cDfL" crossorigin="anonymous"></script>
  </body>
