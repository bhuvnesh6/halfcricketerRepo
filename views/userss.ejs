<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
    <link rel="stylesheet" href="/stylesheets/userlist.css">
</head>
<body>
    <div class="sear">
        <form id="search" action="/Uresult" method="post">
            <input type="text" name="search" id="item" placeholder="Seacrh anyone">
            <button type="submit" id="subtn">search</button>
        </form>
    </div>
    <div class="list-wrapper" ng-app="app" ng-controller="MainCtrl as ctrl">
        <ul class="list">
           <% user.forEach(data => {%>
          <li class="list-item">
            <div>
            <img src="/images/profiles/<%=data.dp%>" class="list-item-image">
            </div>
            <div class="list-item-content">
              <h4><a href="/userS/<%=data._id%>"><%=data.name%></a></h4>
              <p>@<%=data.username%></p>
            </div>
          </li>
        <%})%>
        </ul>
      </div>
</body>
</html>